{
  "name": "mobx-websocket-store",
  "version": "0.2.0",
  "description": "MobX store generator to create stores that use atoms to open and close websocket connections based on the observed state of the store",
  "repository": {
    "type": "git",
    "url": "https://github.com/sampsonjoliver/mobx-websocket-store"
  },
  "bugs": {
    "url": "https://github.com/sampsonjoliver/mobx-websocket-store/issues"
  },
  "homepage": "https://github.com/sampsonjoliver/mobx-websocket-store#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "predemo": "yarn build:test",
    "demo": "node ./lib/demo/testRunner",
    "prebuild": "rimraf lib",
    "prebuild:test": "rimraf lib",
    "build:test": "tsc -p tsconfig.demo.json",
    "build": "yarn tsc"
  },
  "keywords": [
    "mobx",
    "websocket",
    "javascript"
  ],
  "files": [
    "lib/",
    "index.d.ts",
    "mobxAtomStore.d.ts"
  ],
  "author": "Sampson J Oliver",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^8.0.31",
    "@types/react": "^16.3.12",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.0-rc"
  },
  "dependencies": {
    "firebase": "^4.13.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "react": "^16.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
